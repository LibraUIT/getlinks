<div class="row">
    <div class="col-lg-12">
      <%= link_to "<< Back", admin_blogs_path %>
    </div>
    <div class="col-lg-12">
        <h1 class="page-header">New Blog</h1>
    </div>
</div>
<div class="row clients-page">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-body">
                <%= form_for(@blog, method: :post, :url => admin_blogs_path, :html => { :role => 'form'}) do |f| %>
                  <% if @blog.errors.full_messages.present? %>
                    <ul class="list-message-error alert alert-danger">
                        <% @blog.errors.full_messages.each do |error_message| %>
                        <li><%= error_message %></li>
                        <% end %>
                    </ul>
                  <% end %>
                <%= render "form", f: f %>
                <div class="col-sm-12">
                    <div class="col-sm-6">
                      <button class="btn btn-primary" type="submit">Submit</button>
                    </div>
                </div>
                <% end %>
            </div>
        </div>
    </div>
</div>
